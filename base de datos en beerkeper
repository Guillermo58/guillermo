SELECT * FROM vuelo;
SELECT COUNT (*) FROM vuelo;
SELECT AVG(duracion) FROM vuelos;
SELECT origen, destino, duracion, FROM vuelos ORDER BY duracion LIMIT 3 ;//este hace que en la se seleccione la tipo de columna en ka que se tiene dentro en el limit.

